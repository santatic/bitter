{
  "name": "bitter",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "dev": "npm run build && node --inspect-brk=9229 dist/server.js",
    "dev-test": "npm run build && node --inspect-brk=9229 dist/test/index.js",
    "build": "npm run tslint && npm run build-ts",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "build-ts": "tsc",
    "watch-debug": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server.ts",
    "debug": "npm run build && npm run watch-debug",
    "test": "npm run build && node dist/test/index.js",
    "watch-test": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/test/index.ts",
    "build-prod": "webpack --config webpack.config.js",
    "prod": "node dist/server.js",
    "clear": "rm -rf dist"
  },
  "bin": "dist/server.bundle.js",
  "pkg": {
    "scripts": "dist/**/*.js",
    "targets": [
      "node9"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "boom": "^7.2.0",
    "ccxt": "^1.14.118",
    "compression": "^1.7.2",
    "connect-mongo": "^2.0.1",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "ethereumjs-tx": "^1.3.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "imap": "^0.8.19",
    "inbox": "^1.1.59",
    "lodash": "^4.17.10",
    "mongoose": "^5.1.3",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.9.0",
    "node": "^9.11.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "path": "^0.12.7",
    "redis": "^2.8.0",
    "request": "^2.87.0",
    "rxjs": "^5.5.11",
    "talib": "^1.0.4",
    "tulind": "^0.8.10",
    "uuid": "^3.2.1",
    "web3": "1.0.0-beta.15",
    "winston": "^2.4.2",
    "ws": "^3.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/boom": "^7.2.0",
    "@types/compression": "0.0.35",
    "@types/connect-mongo": "0.0.34",
    "@types/crypto-js": "^3.1.40",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.9",
    "@types/express-validator": "^3.0.0",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^2.2.48",
    "@types/mongodb": "^2.2.20",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.10.18",
    "@types/passport": "^0.4.5",
    "@types/redis": "^2.8.6",
    "@types/request": "^2.47.0",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.9",
    "@types/ws": "^3.2.1",
    "js-beautify": "^1.7.5",
    "node-loader": "^0.6.0",
    "nodemon": "^1.17.5",
    "ts-loader": "^3.5.0",
    "ts-node": "^3.3.0",
